{
  "name": "OrganizeMyTabs",
  "manifest_version": 2,
  "short_name": "OrganizeMyTabs",
  "description": "This extension organizes your tabs for you.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "sort-128.png",
    "default_popup": "popup.html",
    "default_title": "OrganizeMyTabs"
  },
  "icons": {
    "128": "sort-128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
